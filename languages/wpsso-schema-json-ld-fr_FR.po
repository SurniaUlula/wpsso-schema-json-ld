#: lib/std/admin/meta-edit.php:76
msgctxt "option comment"
msgid "days"
msgstr "jours"

#: lib/gettext-pro.php:7 lib/pro/admin/meta-edit.php:97
#: lib/std/admin/meta-edit.php:77
msgctxt "option comment"
msgid "hours"
msgstr "heures"

#: lib/gettext-pro.php:8 lib/pro/admin/meta-edit.php:98
#: lib/std/admin/meta-edit.php:78
msgctxt "option comment"
